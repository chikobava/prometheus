{"dashboard":
{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "hideControls": false,
  "id": null,
  "links": [],
  "rows": [
      {
      "collapse": false,
      "height": "250px",
      "panels": [],
      "repeat": null,
      "repeatIteration": null,
      "repeatRowId": null,
      "showTitle": false,
      "title": "Dashboard Row",
      "titleSize": "h6"
    }
  ],
  "schemaVersion": 14,
  "style": "dark",
  "tags": [],
  "templating": {
    "list": [
      {
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        },
        "hide": 0,
        "label": null,
        "name": "data_source",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "Prometheus",
        "type": "datasource"
      },
      {
        "allValue": null,
        "current": {
          "tags": [],
          "text": "[% groups['prom_nodes']| random  %]",
          "value": "[% groups['prom_nodes']| random  %]"
        },
        "hide": 0,
        "includeAll": false,
        "label": null,
        "multi": false,
        "name": "hostname",
        "options": [
{%-for host in groups['prom_nodes'] %} 
          {
            "selected": false,
            "text": "[%host%]",
            "value": "[%host%]"
          },
{%-endfor%}

          {
            "selected": true,
            "text": "",
            "value": ""
          }
        ],
        "query": "{%-for host in groups['prom_nodes'] %}[%host%], {%-endfor%}",
        "type": "custom"
      },
      {
        "current": {
          "value": "9100",
          "text": "9100"
        },
        "hide": 0,
        "label": "node-exporter port",
        "name": "port",
        "options": [
          {
            "value": "9100",
            "text": "9100"
          }
        ],
        "query": "9100",
        "type": "constant"
      }
    ]
  },
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "time_options": [
      "5m",
      "15m",
      "1h",
      "6h",
      "12h",
      "24h",
      "2d",
      "7d",
      "30d"
    ]
  },
  "timezone": "",
  "title": "ChangeMe",
  "version": 1
},
"overwrite": true
}
